<div class="boxforforms">
<h3>Listing of table headers</h3>
<table id="ResourceTable" class="table table-striped">
  <tr>
    <th>Resource type</th>
    <th>Table template</th>
    <th>Orientation</th>
    <th>Position num</th>
    <th>Parent</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @table_headers.each do |table_header| %>
  <tr>
    <td><%= table_header.resource_type_id %></td>
    <td><%= table_header.table_template_id %></td>
    <td><%= table_header.orientation %></td>
    <td><%= table_header.position_num %></td>
    <td><%= table_header.parent_id %></td>
    <td><%= link_to 'Show', table_header, :class => "btn btn-info" %></td>
    <td><%= link_to 'Edit', edit_table_header_path(table_header), :class => "btn btn-success" %></td>
    <td><%= link_to 'Destroy', table_header, method: :delete, :class => "btn btn-danger", data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

</div>